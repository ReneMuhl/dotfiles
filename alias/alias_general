#haskell
alias yd='yesod devel'

#python
alias py='python'
alias py3='python3'

#gpg
alias gpge='gpg --encrypt -a --recipient $1 $2'
alias gpgd='gpg --decrypt $1'

#other
alias ..='cd ..'
alias g='git'
alias gs='grep -isr '$1' '$2''
alias hgrep='history |grep'
alias ll='ls -ahls'

function mkcd(){
      mkdir -p $1
        cd $1
}
